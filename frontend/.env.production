# Frontend Production Environment Variables
# These will be embedded in the build, so only include public-safe values

# API Endpoints
REACT_APP_API_URL=https://your-backend-domain.com
REACT_APP_WS_URL=wss://your-backend-domain.com

# App Configuration
REACT_APP_NAME=AI Trading Platform
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=production

# Feature Flags
REACT_APP_ENABLE_DEMO_MODE=true
REACT_APP_ENABLE_REAL_TRADING=false
REACT_APP_ENABLE_NOTIFICATIONS=true

# Analytics (if using)
REACT_APP_GOOGLE_ANALYTICS_ID=your-ga-id
REACT_APP_MIXPANEL_TOKEN=your-mixpanel-token

# External Services
REACT_APP_SENTRY_DSN=your-frontend-sentry-dsn

# Build Configuration
GENERATE_SOURCEMAP=false
INLINE_RUNTIME_CHUNK=false